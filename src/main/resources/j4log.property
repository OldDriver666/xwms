#J4LOG版本号
j4log.version=2

#J4LOG日志默认根目录
j4log.root=${catalina.home}/logs/business_log

#默认日志文件路径
_j4log=business,${catalina.home}/logs/business_log/default/_j4log.log

#日志级别
j4log.level = DEBUG

#服务使用的业务日志
restApiPerformance=business,${catalina.home}/logs/business_log/performance/performance
authority=info,${catalina.home}/logs/business_log/authority/authority
doFilterInternal=info,${catalina.home}/logs/business_log/filter/doFilterInternal